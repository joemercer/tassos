    "node": "0.10.x"
  },
  "scripts": {
    "start": "node server.js"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "jquery": "./bower_components/jquery/jquery.js",
    "lodash": "./node_modules/lodash/dist/lodash.compat.js"
  },
  "browserify-shim": "./shims.js",
  "dependencies": {
    "errorHandler": "~1.0.1",
    "express": "~4.4.1",
    "lodash": "~2.4.1",
    "morgan": "~1.1.1"
